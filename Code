#include <iostream>
using namespace std;
#include <ctype.h>
#include <cstring>
#include <string>


int start()
{
    int b, y, g, oj, p, bk, wh;
    
    srand(time(0));
    
    b = 0 + ((rand() % 3) + 1);
    y = 0 + ((rand() % 3) + 1);
    g = 0 + ((rand() % 3) + 1);
    oj = 0 + ((rand() % 3) + 1);
    p = 0 + ((rand() % 3) + 1);
    
    bk = 16 - ((rand() % 3) + 1);
    wh = 16 - ((rand() % 3) + 1);
    
    cout << " Blue is at " << b << "\n";
    cout << " Yellow is at " << y << "\n";
    cout << " Green is at " << g << "\n";
    cout << " Orange is at " << oj << "\n";
    cout << " Purple is at " << p << "\n";
    cout << " Black is at " << bk << "\n";
    cout << " White is at " << wh << "\n";
   
    return b, y, g, oj, p, bk, wh;
}
